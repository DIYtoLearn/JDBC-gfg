package exceptions;

class Demo
{
	static Demo d;
	
	private Demo()
	{
		
	}
	
	public static Demo getDemo()
	{
		return new Demo();
	}
	
}

public class SingleTonDesignPatternDemo 
{
	public static void main(String[] args) {
		Demo d1 =Demo.getDemo();
		Demo d1 =Demo.getDemo();
		Demo d1 =Demo.getDemo();
	}

}
